CREATE TABLE Lieu (
    id_lieux INTEGER PRIMARY KEY,
    atmosphere TEXT,
    latitude REAL,
    longitude REAL,
    categorie_route TEXT,
    surface TEXT
);
CREATE TABLE Accident (
    Num_acc INTEGER PRIMARY KEY,
    id_lieux INTEGER,
    choc TEXT,
    gravite INTEGER,
    securite TEXT,
    sens_circulation TEXT,
    collision TEXT,
    FOREIGN KEY (id_lieux) REFERENCES Lieu(id_lieux)
);
CREATE TABLE Vehicule (
    id_vehicule INTEGER PRIMARY KEY,
    categorie_vehicule TEXT,
    moteur TEXT,
    manoeuvre TEXT
);
CREATE TABLE Usager (
    id_usager INTEGER PRIMARY KEY,
    id_vehicule INTEGER,
    place_vehicule INTEGER,
    sexe TEXT,
    annee_naissance INTEGER,
    FOREIGN KEY (id_vehicule) REFERENCES Vehicule(id_vehicule)
);
